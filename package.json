    "react-i18next": "^13.4.0",
    "react-router-dom": "^6.16.0"
  },
  "loki": {
    "looks-same": {
      "ingnoreCaret": true
    },
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    },
    "chromeDockerUseCopy": true
  }
